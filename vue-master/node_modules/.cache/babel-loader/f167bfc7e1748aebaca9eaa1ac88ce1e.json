{"ast":null,"code":"import \"core-js/modules/es.json.stringify.js\";\nimport \"core-js/modules/es.regexp.exec.js\";\nimport \"core-js/modules/es.string.replace.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport { setRoutes } from \"@/router\";\nexport default {\n  name: \"Login\",\n  data: function data() {\n    return {\n      user: {},\n      rules: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 10,\n          message: '长度在 3 到 10 个字符',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }, {\n          min: 6,\n          max: 20,\n          message: '长度在 6 到 20 个字符',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {\n    login: function login() {\n      var _this = this;\n\n      this.$refs['userForm'].validate(function (valid) {\n        if (valid) {\n          // 表单校验合法\n          _this.request.post(\"/user/login\", _this.user).then(function (res) {\n            if (res.code === '200') {\n              localStorage.setItem(\"user\", JSON.stringify(res.data)); // 存储用户信息到浏览器\n\n              localStorage.setItem(\"menus\", JSON.stringify(res.data.menus)); // 动态设置当前用户的路由\n\n              setRoutes();\n\n              _this.$router.replace(\"/home\");\n\n              _this.$message.success(\"登录成功\");\n            } else {\n              _this.$message.error(res.msg);\n            }\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8BA;AACA;AACAA,eADA;AAEAC,MAFA,kBAEA;AACA;AACAC,cADA;AAEAC;AACAC,mBACA;AAAAC;AAAAC;AAAAC;AAAA,SADA,EAEA;AAAAC;AAAAC;AAAAH;AAAAC;AAAA,SAFA,CADA;AAKAG,mBACA;AAAAL;AAAAC;AAAAC;AAAA,SADA,EAEA;AAAAC;AAAAC;AAAAH;AAAAC;AAAA,SAFA;AALA;AAFA;AAaA,GAhBA;AAiBAI;AACAC,SADA,mBACA;AAAA;;AACA;AACA;AAAA;AACA;AACA;AACAC,qEADA,CACA;;AACAA,4EAFA,CAGA;;AACAC;;AACA;;AACA;AACA,aAPA,MAOA;AACA;AACA;AACA,WAXA;AAYA;AACA,OAfA;AAgBA;AAlBA;AAjBA","names":["name","data","user","rules","username","required","message","trigger","min","max","password","methods","login","localStorage","setRoutes"],"sourceRoot":"src/views","sources":["Login.vue"],"sourcesContent":["<template>\n  <div class=\"wrapper\" >\n    <div style=\"height: 0px;opacity: 0.5;\" >\n      <h2 style=\"color: #E6A23C;position:relative;right: -70px;bottom: -8px;background-color: black;width: 196px;height: 42px;line-height: 42px\">企业员工管理系统</h2>\n      <img src=\"../assets/zr.jpeg\" alt=\"\" width=\"57\" style=\"position: relative;top: -35px;right:-13px;opacity: 0.8\" >\n    </div>\n    <div style=\"margin: 200px auto; background-color: black;opacity:0.5;width: 350px; height: 300px; padding: 20px; border-radius: 10px\">\n      <div style=\"opacity:1;margin: 20px 0; text-align: center; font-size: 24px;color: #9713cb;font-weight: bolder\"><b>Login</b></div>\n      <el-form :model=\"user\" :rules=\"rules\" ref=\"userForm\" >\n        <el-form-item prop=\"username\">\n          <el-input size=\"medium\" style=\"margin: 10px 0\" prefix-icon=\"el-icon-user\" v-model=\"user.username\"></el-input>\n        </el-form-item>\n        <el-form-item prop=\"password\">\n          <el-input size=\"medium\" style=\"margin: 10px 0\" prefix-icon=\"el-icon-lock\" show-password v-model=\"user.password\"></el-input>\n        </el-form-item>\n        <el-form-item style=\"margin: 10px 0; text-align: right\">\n          <el-button type=\"warning\" plain size=\"small\"  autocomplete=\"off\" @click=\"$router.replace('/register')\">前往注册</el-button>\n          <el-button type=\"primary\" plain size=\"small\"  autocomplete=\"off\" @click=\"login\">登录</el-button>\n        </el-form-item>\n        <div>\n          <router-link to=\"/front\" style=\"text-decoration: none;color: blue\">\n            返回官网\n          </router-link>\n        </div>\n      </el-form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {setRoutes} from \"@/router\";\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      user: {},\n      rules: {\n        username: [\n          { required: true, message: '请输入用户名', trigger: 'blur' },\n          { min: 3, max: 10, message: '长度在 3 到 10 个字符', trigger: 'blur' }\n        ],\n        password: [\n          { required: true, message: '请输入密码', trigger: 'blur' },\n          { min: 6, max: 20, message: '长度在 6 到 20 个字符', trigger: 'blur' }\n        ],\n      }\n    }\n  },\n  methods: {\n    login() {\n      this.$refs['userForm'].validate((valid) => {\n        if (valid) {  // 表单校验合法\n          this.request.post(\"/user/login\", this.user).then(res => {\n            if(res.code === '200') {\n              localStorage.setItem(\"user\", JSON.stringify(res.data))  // 存储用户信息到浏览器\n              localStorage.setItem(\"menus\", JSON.stringify(res.data.menus))\n              // 动态设置当前用户的路由\n              setRoutes()\n              this.$router.replace(\"/home\")\n              this.$message.success(\"登录成功\")\n            } else {\n              this.$message.error(res.msg)\n            }\n          })\n        }\n      });\n    }\n  }\n}\n</script>\n\n<style>\n.wrapper {\n  height: 100vh;\n  /*background-image: linear-gradient(to bottom right, #FC466B , #3F5EFB);*/\n  overflow: hidden;\n  background-image: url(\"../assets/zr3.jpg\");\n  background-size: 100%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}
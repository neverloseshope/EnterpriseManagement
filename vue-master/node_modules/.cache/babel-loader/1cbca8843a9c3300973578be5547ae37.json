function (res) {\n        if (res.data) {\n          _this3.$message.success(\"删除成功\");\n\n          _this3.pageNum = 1;\n\n          _this3.load();\n        } else {\n          _this3.$message.error(\"删除失败\");\n        }\n      });\n    },\n    handleSelectionChange: function handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    delBatch: function delBatch() {\n      var _this4 = this;\n\n      var ids = this.multipleSelection.map(function (v) {\n        return v.id;\n      }); // [{}, {}, {}] => [1,2,3]\n\n      this.request.post(\"/menu/del/batch\", ids) // 这里使用delete会报错\n      .then(function (res) {\n        if (res.data) {\n          _this4.$message.success(\"批量删除成功\");\n\n          _this4.pageNum = 1;\n\n          _this4.load();\n        } else {\n          _this4.$message.error(\"批量删除失败\");\n        }\n      });\n    },\n    reset: function reset() {\n      this.name = \"\";\n      this.load();\n    },\n    handleAdd: function handleAdd(id) {\n      this.dialogFormVisible = true;\n      this.form = {};\n\n      if (id) {\n        this.form.pid = id;\n      }\n    },\n    handleAddMater: function handleAddMater() {\n      this.masterDialogFormVisible = true;\n      this.form = {};\n    },\n    handleEdit: function handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row)); //可以解决编辑未确认就改变的问题\n\n      this.dialogFormVisible = true;\n    },\n    handleSizeChange: function handleSizeChange(pageSize) {\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange: function handleCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuHA;AACAA,cADA;AAEAC,MAFA,kBAEA;AACA;AACAC,mBADA;AAEAC,cAFA;AAGAC,gBAHA;AAIAC,kBAJA;AAKAL,cALA;AAMAM,cANA